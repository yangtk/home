@charset "utf-8";
/* CSS Document */

/*body,html {	width:100%;	height:100%; overflow:hidden; }*/
.screen1 { width:100%;-height:100%; height:550px; background:#01a0ca; background-size:12px; position:relative; overflow:hidden;
}
.screen1 .bg { width:100%; height:550px; background:url(../css/images/bg.png); opacity:0.15; position:absolute; }
.screen1 .head { padding:20px 20px 20px 40px; }
.screen1 .logo { width:240px; height:100px; background:url(../css/images/logo.png) no-repeat; -background-size:210px; }
.screen1 .welcome { height:22px; font-size:12px; font-family:宋体; opacity:0.6; }
.screen1 .nav { width:calc(100% - 420px); min-width:60%; padding-left:65px; background-color:rgba(0,0,0,0.1); z-index:9; position:fixed; right:22px; top:43px; font-size:0; }
.screen1 .nav ul { display:inline-block; }
.screen1 .nav ul li { float:left; }
.screen1 .nav ul li a { display:inline-block; padding:10px 0; width:130px; font-size:15px; text-shadow:1px 1px 1px white; text-align:center; border-top:3px solid transparent; }
.screen1 .nav ul li a:hover { background-color:rgba(0,0,0,0.1); }
.screen1 .nav ul li a.on { background-color:rgba(0,0,0,0.1); border-top:3px solid #ffa800; }
@media screen and (min-width:900px) and (max-width:1100px){
	.screen1 .nav { padding-left:25px; width:calc(100% - 340px); }
	.screen1 .nav ul li a { width:100px; }
}
@media screen and (min-width:500px) and (max-width:900px){
	.screen1 .nav { padding-left:25px; width:calc(100% - 340px); }
	.screen1 .nav ul li a { width:60px; font-size:14px; }
}




.screen1 .nav.nav_fixed { position:fixed; top:0; right:0; width:100%; text-align:center; }
/**首屏底部部分**/
.screen1 .down { width:100%; position:absolute; bottom:0; overflow:hidden; } 
.screen1 .down p { width:100%; height:175px; }
.screen1 .down .p1 { background:url(../css/images/footer-yuan.png) no-repeat; background-size:128%; background-position:-200px 120px; 
animation:p1 30s infinite;
-webkit-animation:p1 30s infinite;
-moz-animation:p1 30s infinite;
}
.screen1 .down .p2 { background:url(../css/images/footer-yuan2.png) no-repeat; position:absolute; bottom:-20px; background-size:100%; background-position:0 55px; 
animation:p2 20s infinite;
-webkit-animation:p2 20s infinite;
-moz-animation:p2 20s infinite;
}
.screen1 .down .p3 { background:url(../css/images/footer-yuan1.png) no-repeat; position:absolute; bottom:10px; background-size:100%; background-position:0 107px; 
animation:p3 14s infinite;
-webkit-animation:p3 14s infinite;
-moz-animation:p3 14s infinite;
}

@keyframes p1 {
	0%	  { background-position:-150px 120px; }
	25%   { background-position:0 115px; }
	50%   { background-position:-100px 125px; }
	75%   { background-position:0 135px; }
	100%  { background-position:-150px 120px; }
	
}
@-webkit-keyframes p1 {
	0%	  { background-position:-150px 120px; }
	25%   { background-position:0 115px; }
	50%   { background-position:-100px 125px; }
	75%   { background-position:0 135px; }
	100%  { background-position:-150px 120px; }
}
@-moz-keyframes p1 {
	0%	  { background-position:-150px 120px; }
	25%   { background-position:0 115px; }
	50%   { background-position:-100px 125px; }
	75%   { background-position:0 135px; }
	100%  { background-position:-150px 120px; }
}

@keyframes p2 {
	0%	  { background-position:0 55px; }
	25%   { background-position:-20px 50px; }
	50%   { background-position:0 55px; }
	75%   { background-position:20px 50px; }
	100%  { background-position:0 55px; }
	
}
@-webkit-keyframes p2 {
	0%	  { background-position:0 55px; }
	25%   { background-position:-20px 50px; }
	50%   { background-position:0 55px; }
	75%   { background-position:20px 50px; }
	100%  { background-position:0 55px; }
}
@-moz-keyframes p2 {
	0%	  { background-position:0 55px; }
	25%   { background-position:-20px 50px; }
	50%   { background-position:0 55px; }
	75%   { background-position:20px 50px; }
	100%  { background-position:0 55px; }
}

@keyframes p3 {
	0%	  { background-position:0 107px; }
	25%   { background-position:-30px 110px; }
	50%   { background-position:0 107px; }
	75%   { background-position:30px 100px; }
	100%  { background-position:0 107px; }
	
}
@-webkit-keyframes p3 {
	0%	  { background-position:0 107px; }
